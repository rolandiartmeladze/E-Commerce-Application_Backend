{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/about",
        "destination": "/about.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          }
        ]
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "API_KEY": "your-api-key"
    },
    "builds": [
      { "src": "/public/**", "use": "@vercel/static" }
    ]
  }
  